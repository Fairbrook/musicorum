<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane id="AnchorPane" prefHeight="250.0" prefWidth="500.0" styleClass="contentPane" stylesheets="@css/general.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="musicorum.views.FXMLNewUnidadController">
   <children>
      <VBox alignment="CENTER" layoutX="58.0" layoutY="19.0" spacing="50.0">
         <children>
            <Label text="Nueva Unidad">
               <styleClass>
                  <String fx:value="txtColor" />
                  <String fx:value="txtHeaderInput" />
               </styleClass>
            </Label>
            <HBox spacing="50.0">
               <children>
                  <Label text="Nombre:">
                     <styleClass>
                        <String fx:value="txtColor" />
                        <String fx:value="lbl-medium" />
                     </styleClass>
                  </Label>
                  <TextField fx:id="txtNombre" />
                  <Button mnemonicParsing="false" onAction="#aceptar" styleClass="btn" text="Aceptar" />
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#cancelar" styleClass="btn" text="Cancelar" />
         </children>
      </VBox>
   </children>
</AnchorPane>
